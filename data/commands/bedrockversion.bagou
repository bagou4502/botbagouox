const {SlashCommandBuilder} = require('@discordjs/builders');
const embed = require('../../libs/linkembed').embed.embed;

// Variable
const name = 'Minecraft Bedrock Version Changer';
const bagouurl = 'mcbedrock';
const pterourl = '298';
const price = '5$';
const description = 'This addon allow you do download all Pocketmine version directly from the panel';
const imagefile = 'mcjava.jpg';

module.exports = {
    data: new SlashCommandBuilder()
        .setName('mcbedrock')
        .setDescription(`For get link of my ${name} addon!`),
    execute (interaction) {

        interaction.reply({
            embeds: [embed(name, bagouurl, pterourl, price, description)],
            ephemeral: false,
            files: [`./data/img/${imagefile}`]
        });
    }
};