const {SlashCommandBuilder} = require('@discordjs/builders');


module.exports = {
    data: new SlashCommandBuilder()
        .setName('clear')
        .setDescription('Clear 100 message of the channel'),
    execute (interaction) {

        const {channel} = interaction;
        channel.bulkDelete(100);
        interaction.reply({
            content: 'Removed',
            ephemeral: true
        });
    }
};